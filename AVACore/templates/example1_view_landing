import { AFViewController } from "avacore";


/**
 *
 */
class MainViewController extends AFViewController {

  constructor(request, response) {
    super(request, response);
  }


  willLoad(request, response, self) {
    self.layout = "app";
    self.template = "landing";
    super.willLoad(request, response, self);
  }

}


export default MainViewController;