{
  "name": "Example 2",
  "type": "BOILERPLATE",
  "description": "This is a boilerplate that installs the most minimal files into Avalanche project to get started.",
  "aliases": [
    "basic",
    "example2"
  ],
  "files": [
    {
      "template": "ava_public_js",
      "path": "/app/public/assets/vendor/Avalanche/js/avalanche.js"
    },
    {
      "template": "ava_layout",
      "path": "/app/templates/layouts/layout.hbs"
    },
    {
      "template": "example2_environment",
      "path": "/app/environments/development.environment.json"
    },
    {
      "template": "example2_controller",
      "path": "/app/controllers/PostController.js"
    },
    {
      "template": "example2_routes",
      "path": "/app/routes/api.json"
    }
  ]
}