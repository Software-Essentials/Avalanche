{
  "name": "Empty",
  "type": "BOILERPLATE",
  "description": "This is a boilerplate that installs nothing at all except for an environment file.",
  "aliases": [
    "empty"
  ],
  "files": [
    {
      "template": "ava_public_js",
      "path": "/app/public/assets/vendor/Avalanche/js/avalanche.js"
    },
    {
      "template": "ava_layout",
      "path": "/app/templates/layouts/layout.hbs"
    },
    {
      "template": "empty_environment",
      "path": "/app/environments/development.environment.json"
    }
  ]
}